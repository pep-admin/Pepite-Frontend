{
  "name": "pepite-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx check-node-version --node 18.18.0",
    "audit": "npm audit",
    "check-updates": "ncu -u",
    "get-git-info": "node set-git-info.cjs",
    "build": "npm run get-git-info && tsc && vite build",
    "changelog": "standard-version",
    "dev": "npm run get-git-info && vite",
    "lint": "eslint \"src/**/*.{js,ts,jsx,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,jsx,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.9",
    "@mui/material": "^5.14.10",
    "@react-spring/rafz": "^9.7.3",
    "@tanstack/react-query": "^4.35.3",
    "axios": "^1.5.0",
    "canvas-confetti": "^1.9.0",
    "fontfaceobserver": "^2.3.0",
    "formik": "^2.4.5",
    "git-rev-sync": "^3.0.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-spring": "^9.7.3",
    "react-youtube": "^10.1.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.2",
    "@types/fontfaceobserver": "^2.1.1",
    "@types/node": "^20.7.1",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "npm-check-updates": "^16.14.4",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}
